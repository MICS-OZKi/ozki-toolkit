{
  "name": "ozki-lib",
  "private": true,
  "version": "1.1.5",
  "description": "ozki-lib zkp project",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "commonjs",
  "files": [
    "index.js",
    "index.d.ts",
    "/dist/**/*",
    "/typings/**/*"
  ],
  "workspaces": [
    "@ozki-lib/generator",
    "@ozki-lib/verifier",
    "@ozki-lib/common"
  ],
  "scripts": {
    "clean": "rm -rf ./dist && rm -rf ./index.js && rm -rf ./index.d.ts & rm -f *.tgz",
    "build": "tsc --build",
    "copy": "cp -r proof-generator/static/ ./dist/proof-generator/static && cp -r proof-verifier/static/ ./dist/proof-verifier/static",
    "prepare": "npm run clean && npm run build && npm run copy",
    "build-circom": "./ozki-toolkit.sh zkp/build/ProvePayPalSubscriptionMain.circom",
    "build-pkg": "npm run clean && npm run build && npm pack"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MICS-OZKi/ozki-toolkit.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MICS-OZKi/ozki-toolkit/issues"
  },
  "homepage": "https://github.com/MICS-OZKi/ozki-toolkit#readme",
  "dependencies": {
    "chai": "^4.3.6",
    "circomlibjs": "^0.1.2",
    "ffjavascript": "^0.2.54",
    "mongodb": "^4.7.0",
    "path": "^0.12.7",
    "snarkjs": "^0.4.18",
    "typescript": "^4.7.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/node": "^17.0.42",
    "ejs-compiled-loader": "^3.1.0",
    "ts-loader": "~9.3.0",
    "tsconfig-paths-webpack-plugin": "~3.5.0",
    "webpack": "~5.73.0",
    "webpack-cli": "^4.9.2"
  }
}
